{
  "name": "trilstack/tril-cli",
  "description": "CLI for the TRIL stack",
  "license": "MIT",
  "homepage": "https://github.com/trilstack/tril-cli",
  "keywords": [],
  "require": {
    "php": "^8.1",
    "nunomaduro/termwind": "^1.15",
    "league/flysystem": "^3.12",
    "symfony/console": "^6.2",
    "symfony/finder": "^6.2",
    "symfony/process": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0",
    "symplify/easy-coding-standard": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "TrilStack\\Cli\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TrilStack\\Cli\\Tests\\": "tests"
    }
  },
  "bin": ["bin/tril"]
}
